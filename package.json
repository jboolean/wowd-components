{
  "name": "wowd-components",
  "version": "0.0.1",
  "description": "Audio components for takomaradio.org",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack  --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "watch:hot": "webpack-dev-server --config webpack.dev.js --hotOnly",
    "lint": "eslint --ext .js --ext jsx src",
    "lint-fix": "eslint --ext .js --ext jsx --fix src",
    "flow": "flow",
    "profile": "webpack --profile --json --config webpack.prod.js > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "install-libdefs": "flow-typed install --ignoreDeps dev --overwrite"
  },
  "keywords": [
    "wowd-lp",
    "fm",
    "community radio",
    "audio"
  ],
  "author": "Julian Boilen",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "eventemitter3": "^3.0.0",
    "fuzzy": "^0.1.3",
    "lodash": "^4.17.10",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.1.1",
    "normalize.css": "^7.0.0",
    "rc-trigger": "^2.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-squarespace": "^1.0.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-react": "^7.5.1",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.62.0",
    "flow-typed": "^2.2.3",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "postcss-loader": "^2.0.9",
    "svg-react-loader": "^0.4.5",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
