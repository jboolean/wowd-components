{
  "name": "wowd-components",
  "version": "1.0.1",
  "description": "Audio components for takomaradio.org",
  "main": "dist/app.bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack  --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "watch:hot": "webpack-dev-server --config webpack.dev.js --hotOnly",
    "lint": "eslint --ext .js --ext jsx src",
    "lint-strict": "eslint --ext .js --ext jsx --max-warnings 0 src",
    "lint-fix": "eslint --ext .js --ext jsx --fix src",
    "flow": "flow",
    "profile": "webpack --profile --json --config webpack.prod.js > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "install-libdefs": "flow-typed install --ignoreDeps dev --overwrite",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard",
    "preversion": "npm run lint-strict && npm run flow"
  },
  "keywords": [
    "wowd-lp",
    "fm",
    "community radio",
    "audio"
  ],
  "author": "Julian Boilen",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "css-scroll-snap-polyfill": "^0.1.2",
    "eventemitter3": "^3.1.0",
    "fuzzy": "^0.1.3",
    "lodash": "^4.17.10",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.2",
    "rc-trigger": "^2.5.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^4.3.1",
    "ua-parser-js": "^0.7.18"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-squarespace": "^1.0.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-react": "^7.10.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.5.1",
    "history": "git://github.com/amuzalevskiy/history.git",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.0.0-rc.15",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "modernizr": "^3.6.0",
    "modernizr-loader": "^1.0.1",
    "postcss-loader": "^2.1.5",
    "publish-please": "^3.2.0",
    "svg-react-loader": "^0.4.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix --max-warnings 0",
      "flow focus-check",
      "git add"
    ]
  }
}
